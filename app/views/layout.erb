  <script src="/js/application.js"></script>

  <title>Hacker News Clone Stuff</title>
</head>
<body>
  <center>
    <table id="hnmain" border="0" cellpadding="0" cellspacing="0" width="85%" bgcolor="#f6f6ef">
      <tbody>
        <tr>
          <td bgcolor="#ff6600">
            <table border="0" cellpadding="0" cellspacing="0"width="100%" style="padding:2px">
              <tbody>
                <tr>
                  <td style="width:18px;padding-right:4px">
                    <a href="">
                      <img src="https://news.ycombinator.com/y18.gif" width="18" height="18" style="border:1px #ffffff solid;">
                    </a>
                  </td>
                  <td style="text-align:left;padding-right:4px;color:black;font-weight: bold">Hacker News Clone
                  </td>
                  <td style="text-align:right;padding-right:4px;">
                    <span class="pagetop">
                      <% if session[:user_id] %>
                      <a href="/users/<%= @user.id %>">
                        <%= @user.name %>
                      </a>
                      <a href="/users/logout">logout</a>
                        <% else %>
                        <a href="users/new"> Sign Up / Login</a>
                        <% end %></a>
                    </span>
                  </td>
                </tr>
              </tbody>
            </table>
          </td>
        </tr>
      </tbody>
    </table>
  </center>
    <table>
      <tbody stype="background-color: #f6f6ef">


        <%= yield %>

      </tbody>
    </table>
  </center>


</body>
